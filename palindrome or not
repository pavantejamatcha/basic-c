#include<stdio.h>
#include<string.h>
#include<conio.h>
void main()
{
    char a[20]="malayalam";
    int l=strlen(a)-1;
    for(int i=0,j=l;i<=j;i++,j--)
    {
        if(a[i]!=a[j])
        {
            printf("It is not a palindrome");
            return;
        }

    }
    printf("it is a palindrome");
}
